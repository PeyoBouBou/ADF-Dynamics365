{
	"name": "pipeline Azure to CDS",
	"properties": {
		"activities": [
			{
				"name": "Copy data1",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "AzureSqlSource"
					},
					"sink": {
						"type": "CommonDataServiceForAppsSink",
						"writeBatchSize": 10,
						"writeBehavior": "upsert",
						"ignoreNullValues": false,
						"alternateKeyName": "bpi_interlocuteur_code_key"
					},
					"enableStaging": false,
					"translator": {
						"type": "TabularTranslator",
						"mappings": [
							{
								"source": {
									"name": "Address",
									"type": "String"
								},
								"sink": {
									"name": "emailaddress1",
									"type": "String"
								}
							},
							{
								"source": {
									"name": "FirstName",
									"type": "String"
								},
								"sink": {
									"name": "firstname",
									"type": "String"
								}
							},
							{
								"source": {
									"name": "LastName",
									"type": "String"
								},
								"sink": {
									"name": "lastname",
									"type": "String"
								}
							},
							{
								"source": {
									"name": "AutoId",
									"type": "Int32"
								},
								"sink": {
									"name": "bpi_code_interlocuteur"
								}
							}
						]
					}
				},
				"inputs": [
					{
						"referenceName": "AzureSqlBizAppData_Table_Persons",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "TSP_Sabatier_CDS_Contact",
						"type": "DatasetReference"
					}
				]
			}
		],
		"annotations": []
	}
}